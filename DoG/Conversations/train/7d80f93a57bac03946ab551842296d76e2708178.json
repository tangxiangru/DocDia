  "history": [
    {
      "docIdx": 0, 
      "text": "Hi", 
      "uid": "user1", 
      "utcTimestamp": "2018-02-15T06:57:14.835Z"
    }, 
    {
      "docIdx": 0, 
      "text": "Hi, How are you", 
      "uid": "user2", 
